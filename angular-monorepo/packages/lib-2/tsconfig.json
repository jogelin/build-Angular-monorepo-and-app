{
    "compilerOptions": {
        "baseUrl": ".",
        "declaration": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "module": "es2015",
        "moduleResolution": "node",
        "outDir": "../../dist/lib-2",
        "sourceMap": false,
        "inlineSources": false,
        "target": "es2015",
        "skipLibCheck": true,
        "lib": [
            "es2015",
            "dom"
        ],
        "paths": {
            "@angular/*": [
                "../../node_modules/@angular/*"
            ],
            "rxjs/*": [
                "../../node_modules/rxjs/*"
            ]
        },
        "types": []
    },
    "files": ["./index.ts"],
    "angularCompilerOptions": {
        "annotateForClosureCompiler": true,
        "strictMetadataEmit": true,
        "bundleIndex": "index",
        "importAs": "@bamaa/lib-2"
    }
}